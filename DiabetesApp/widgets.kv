<Main>:
    
    MDNavigationLayout:
        size : root.width, root.height

        ScreenManager:
            id: screen_manager

            Screen:
                name: "home"

                BoxLayout:
                    orientation: 'vertical'

                    MDToolbar:
                        id: toolbar
                        title: "Home"
                        elevation: 10
                        left_action_items: [['menu', lambda x: nav_drawer.set_state("open")]] 

                    FloatLayout:
                        size: root.width, root.height
                        insulinsvar:insulinsvar
                        karbotall:karbotall


                        BoxLayout:
                            orientation: 'vertical'
                            size_hint: (0.2, 0.3)
                            pos_hint: {'top': 0.8, 'x': 0.4}

                            MDTextField:
                                id: karbotall
                                hint_text: "Inusulin calculator"
                                input_filter: 'int'
                                helper_text: "Carbohydrates to Insulin calculator"
                                helper_text_mode: "on_focus"

                            MDRaisedButton:
                                elevation: 10
                                text: 'Kalkulate'
                                on_release: root.kalkulate()
                                size_hint: (1, None)

                            MDLabel:
                                text: ''
                                halign: "center"
                                id: insulinsvar

            Screen:
                name: "settings"

                BoxLayout:
                    orientation: 'vertical'

                    MDToolbar:
                        id: toolbar
                        title: "Settings"
                        elevation: 10
                        left_action_items: [['menu', lambda x: nav_drawer.set_state("open")]] 

                    FloatLayout:
                        size: root.width, root.height

                        MDRaisedButton:
                            text: 'hello'
                            size_hint: (0.3, 0.3)
                            pos_hint: {'x ': 0.3, 'y': 0.5}

            Screen:
                name: "food"

                BoxLayout:
                    orientation: 'vertical'

                    MDToolbar:
                        id: toolbar
                        title: "Drinks and Food"
                        elevation: 10
                        left_action_items: [['menu', lambda x: nav_drawer.set_state("open")]] 
                        right_action_items: [['plus-box', lambda x: root.add_item(x)]]

                    MDTabs:

                        FoodTab:
                            title: "Food"
                            
                        DrinksTab:
                            title: "drinks"

            Screen:
                name: "login"

                BoxLayout:
                    orientation: 'vertical'

                    MDToolbar:
                        id: toolbar
                        title: "Drinks and Food"
                        elevation: 10
                        left_action_items: [['menu', lambda x: nav_drawer.set_state("open")]] 

                    FloatLayout:
                        size: root.width, root.height

            Screen
                name: "add_foods"


        MDNavigationDrawer:
            id: nav_drawer

            ContentNavigationDrawer:
                screen_manager: screen_manager
                nav_drawer: nav_drawer


<ContentNavigationDrawer>:

    cols: 1

    OneLineListItem:
        text: "Your Name"

        size_hint: 1, None

    OneLineAvatarListItem:
        text: "Home"
        on_release:
            root.nav_drawer.set_state("close")
            root.screen_manager.current = "home"

        IconLeftWidget:
            icon: "home"

            on_release:
                root.nav_drawer.set_state("close")
                root.screen_manager.current = "home"


    OneLineAvatarListItem:
        text: "Food"
        on_release:
            root.nav_drawer.set_state("close")
            root.screen_manager.current = "food"

        IconLeftWidget:
            icon: "food"
            on_release:
                root.nav_drawer.set_state("close")
                root.screen_manager.current = "food"

    OneLineAvatarListItem:
        text: "Settings"
        size_hint: 1, None
        on_release:
            root.nav_drawer.set_state("close")
            root.screen_manager.current = "settings"

        IconLeftWidget:
            icon: "account-settings"

            on_release:
                root.nav_drawer.set_state("close")
                root.screen_manager.current = "settings"

<FoodTab>:

<DrinksTab>:

<Add_food>
    orientation: "vertical"
    spacing: "12dp"
    size_hint_y: None
    height: "120dp"

    MDTextField:
        hint_text: "City"

    MDTextField:
        hint_text: "Street"

    


<loginwindow>:
    deltallbl : antalldeltal
    name:name
    Label:
        text: 'Name'
        size_hint: (0.3, 0.3)
        font_size: 80
        pos_hint: {'x': 0.35, 'top': 1.1}

    TextInput:
        id: name
        multiline: False
        size_hint: (0.4, 0.06)
        pos_hint: {'x': 0.3, 'top':0.75}

    Button:
        text: 'Neste'
        pos_hint: {'x': 0.4, 'top': 0.13 }
        size_hint: (None, None)
        size: root.width / 6, root.height / 10
        font_size: 52
        on_release: root.showlogin2()

    Label:
        id: antalldeltal
        size_hint: (0.3, 0.3)
        font_size: 52
        text: ''
        pos_hint: {'x': 0.35, 'top': 0.425}

    Label:
        font_size: 52
        text: 'Antall Delingstall p√• en dag'
        pos_hint: {'x': 0.35 , 'top': 0.65}
        size_hint: (0.3, 0.3)

    Slider:
        min: 1
        max: 6
        value: 1
        step: 1
        orientation: 'horizontal'
        size_hint: (0.6,0.4)
        pos_hint: {'x': 0.2, 'top': 0.6}
        on_value: root.sliderdeltall(*args)

    Button:
        text: 'Avbryt'
        on_release: root.close()
        size_hint: (None, None)
        size: root.width / 8, root.height / 10
        font_size: 52
        pos_hint: {'x': 0, 'top': 0.13}

